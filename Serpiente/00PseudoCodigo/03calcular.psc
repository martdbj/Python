// Programa que lee dos números y musetra la suma, resta, multiplicación y división
Proceso calcular
	Definir num1 Como Real;
	Definir num2 Como Real;
	Escribir 'Inserte dos números';
	Leer num1;
	Leer num2;
	Escribir 'Multiplicion: ', num1 * num2;
	Escribir 'Suma: ', num1 + num2;
	Escribir 'Resta: ', num1 - num2;
	Escribir 'División: ', num1 / num2;
FinProceso
